<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.teleinfo.bidadmin.blockchain.mapper.TrustanchorUpdateVoteMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="trustanchorUpdateVoteResultMap" type="cn.teleinfo.bidadmin.blockchain.entity.TrustanchorUpdateVote">
        <id column="ID" property="id"/>
        <result column="ProposeId" property="ProposeId"/>
        <result column="votebid" property="votebid"/>
        <result column="validity" property="validity"/>
        <result column="voteTime" property="voteTime"/>
    </resultMap>


    <select id="selectTrustanchorUpdateVotePage" resultMap="trustanchorUpdateVoteResultMap">
        select * from bif_trustanchor_update_vote where is_deleted = 0
    </select>

</mapper>
