<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.teleinfo.bidadmin.blockchain.mapper.ResourceMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="resourceResultMap" type="cn.teleinfo.bidadmin.blockchain.entity.Resource">
        <id column="id" property="id"/>
        <result column="create_user" property="createUser"/>
        <result column="create_time" property="createTime"/>
        <result column="update_user" property="updateUser"/>
        <result column="update_time" property="updateTime"/>
        <result column="is_deleted" property="isDeleted"/>
        <result column="status" property="status"/>
        <result column="name" property="name"/>
        <result column="hash" property="hash"/>
        <result column="size" property="size"/>
        <result column="large_Size" property="largeSize"/>
        <result column="type" property="type"/>
        <result column="REMARKS" property="remarks"/>
    </resultMap>


    <select id="selectResourcePage" resultMap="resourceResultMap">
        select * from sys_resource where is_deleted = 0
    </select>

</mapper>
